@page "/terms"

@model BaseTermsModel

@inject IIdentityViewLocalizer Localizer

@{
    ViewData["body-css-class"] = "article-page";
    ViewData["Title"] = Localizer[Model.View.Title];
}

<article class="p-0 py-md-4 px-md-5">
    <vc:page-heading title="" image-src="" />
    <md path="@Model.View.MarkdownPath" class="markdown-body"></md>
</article>