<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Nullable>enable</Nullable>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageTags>AspNetCore;Risk;RulesEngine;MinimalApis</PackageTags>
    <TargetFrameworks>net7.0;net8.0</TargetFrameworks>
    <VersionPrefix>$(VersionPrefixRisk)</VersionPrefix>
  </PropertyGroup>
  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Indice.AspNetCore" Version="$(VersionPrefixCore)" />
    <PackageReference Include="Indice.Features.Risk.Core" Version="$(VersionPrefixRisk)" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Indice.AspNetCore\Indice.AspNetCore.csproj" />
    <ProjectReference Include="..\Indice.Features.Risk.Core\Indice.Features.Risk.Core.csproj" />
  </ItemGroup>
</Project>
